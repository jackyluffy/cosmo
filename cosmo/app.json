{
  "expo": {
    "name": "Cosmo",
    "slug": "cosmo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "cosmo",
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#0a0e27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cosmoapp.datingapp",
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "cosmo",
              "com.cosmoapp.datingapp",
              "com.googleusercontent.apps.691853413697-d3r6po74rsm6ak7vdfk51a764lnk9udv"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.cosmo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
